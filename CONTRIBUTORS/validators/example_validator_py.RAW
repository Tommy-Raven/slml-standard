class ExampleValidator:
    def __init__(self, spec_version):
        self.spec_version = spec_version

    def validate(self, manifest):
        if not isinstance(manifest, dict):
            return {
                "status": "CORRUPTED",
                "reason_code": "PARSE_FAILURE"
            }

        if "entities" not in manifest:
            return {
                "status": "CORRUPTED",
                "reason_code": "MISSING_ENTITIES"
            }

        return {
            "status": "ADMISSIBLE"
        }
